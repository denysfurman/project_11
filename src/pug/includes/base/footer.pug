footer.footer
	.footer__content
		a.footer__logo(
			href="/"
		)
			img.footer__image(
				src="../images/logo_footer.svg",
				alt=""
			)
		nav.navigation.navigation_footer
			.navigation__content.navigation__content_footer
				- var names = ['Home','Tests', 'Student records', 'Forums', 'Articles', 'Q&A Library', 'Certificates', 'Help'];
					each name in names
						a(
							class="navigation__link navigation__link_footer"
							href="#"
						)=name
		.statistics
			- var titles = ['Number of students','Number of teachers','Number of courses','Total number of trainees in all courses','Total number of students currently'];	
			- var numbers = ['1500','50','300','600','500'];	
				each title, key in titles
					.statistics__item
						b=title
						=numbers[key]
		.socials.socials_footer
			- var x = 5;
			- for (var i=1; i<=x; i++) {
				a(
					class="socials__link socials__link_footer socials__link_" + i
					href="#"
					target="_blank"
				)
			- }
		p.footer__copyright
